{
  "name": "opencode-to-ccusage",
  "version": "0.1.0",
  "description": "Export OpenCode sessions to ccusage-compatible JSONL format",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "opencode-to-ccusage": "./src/index.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "repository": {
